{
  "RestServices": {
    "ApiUrl": "https://trello.com/",
    "Timeout": 10
  },
  "SeleniumServices": {
    "ExecutionEnvironment": "Local",
    "Browser": "Chrome",
    "ApplicationUrl": "https://trello.com/",
    "HeadlessMode": "Off",
    "Timeout": 30
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      { "Name": "OpenTracing" }
    ],
    "Enrich": [ "FromLogContext", "WithTransportMessage" ],
    "Properties": {
      "ApplicationName": "Trello.UiTest"
    }
  }
}
