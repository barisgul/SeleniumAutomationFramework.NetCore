{
  "RestServices": {
    "BaseUrl": ""
  },
  "SeleniumServices": {
    "ExecutionEnvironment": "Local",
    "Browser": "Chrome",
    "WebDriverTimeout": 10000
  },

  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Class Library",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      { "Name": "OpenTracing" }
    ],
    "Enrich": [ "FromLogContext", "WithTransportMessage" ],
    "Properties": {
      "ApplicationName": "Trello.UiTest"
    }
  }
}
